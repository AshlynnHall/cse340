<h1 class="acctTitle"><%= title %></h1>

<%- messages() %>

<% if (errors) { %>
    <ul class="notice">
        <% errors.array().forEach(error => { %>
            <% if(error.msg !== "Invalid value") {%>
                <li><%= error.msg %></li>
            <%}%>
        <% }) %>
    </ul>
<% } %>

<h2>You're logged in</h2>